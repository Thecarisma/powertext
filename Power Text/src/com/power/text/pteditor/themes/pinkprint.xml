<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- <!DOCTYPE PowertextTheme SYSTEM "theme.dtd"> -->
 <!-- http://www.powertext.com -->

<PowerTextTheme version="0.0.1" >

	<!-- Basic-Components. -->
	<Basic-Components id="0001" >
		<background>FF9999</background>
		<foreground>FFFFFC</foreground>	
		<border>null</border>
	</Basic-Components> 

	<!-- Tab-Component. -->
	<Tab-Component id="0002" >
		<selected>FF9999</selected>
		<unselected>FFAFA7</unselected>
		<gradient>false</gradient>
	</Tab-Component>

	<!-- PTextArea-Editor. -->
	<PTextArea-Editor id="0003" >
		<caret>D2D2D2</caret>
		<selectionbackground>4C4C4C</selectionbackground>
		<selectionforeground>FF6718</selectionforeground>
		<currentlinehighlight>383A38</currentlinehighlight>
		<currentlinehighlight-fade>true</currentlinehighlight-fade>
		<marginline>394448</marginline>
		<markallhighlight>383A38</markallhighlight>
		<markoccurrencehighlight>383A38</markoccurrencehighlight>
		<markoccurrenceborder>false</markoccurrenceborder>
		<matchbracketbackground>2E2E2E</matchbracketbackground>
		<matchbracketforeground>F0F0F0</matchbracketforeground>
		<matchbracketanimate>true</matchbracketanimate>
		<hyperlinks>A082BD</hyperlinks>
	</PTextArea-Editor>

	 <!-- Secondary-Languages. -->
	<Secondary-Languages id="0004" >
		<language1>0</language1> <language1bg>333344</language1bg> 
		<language2>1</language2> <language2bg>223322</language2bg> 
		<language3>2</language3> <language3bg>332222</language3bg> 
  	 </Secondary-Languages>

   <!-- Syntax tokens. -->
   <tokenStyles id="0005" >
